{
  "name": "v-tree",
  "version": "0.0.1",
  "description": "A Simple tree component for your awesome Vue apps",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinayakkulkarni/v-tree"
  },
  "main": "dist/v-tree.js",
  "module": "dist/v-tree.esm.js",
  "unpkg": "dist/v-tree.js",
  "jsdelivr": "dist/v-tree.js",
  "cdn": "dist/v-tree.js",
  "private": false,
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "test": "ava",
    "test:snapshot": "ava --update-snapshots",
    "build": "npm run bundle && npm run bundle:min",
    "bundle": "rollup -c build/rollup.config.js",
    "bundle:min": "rollup -c build/rollup.min.config.js",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lintfix": "npm run lint:eslint:fix && npm run lint:prettier:fix",
    "lint:eslint": "eslint \"{,!(node_modules|dist|example)/**/}*.{js,vue}\"",
    "lint:eslint:fix": "eslint --fix \"{,!(node_modules|dist|example)/**/}*.{js,vue}\"",
    "lint:prettier": "prettier --check \"{,!(node_modules|dist|example)/**/}*.{js,vue}\"",
    "lint:prettier:fix": "prettier --write \"{,!(node_modules|dist|example)/**/}*.{js,vue}\"",
    "release": "shipjs prepare"
  },
  "dependencies": {
    "@vue/composition-api": "^1.0.0-beta.16"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/core": "^7.12.0",
    "@babel/preset-env": "^7.12.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@vinayakkulkarni/prettier-config-vue": "^1.0.0",
    "@vue/test-utils": "^1.1.0",
    "ava": "^3.13.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.3.0",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "prettier": "^2.1.2",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0",
    "require-extension-hooks-vue": "^3.0.0",
    "rollup": "^2.30.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.6",
    "shipjs": "^0.22.0",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "author": {
    "name": "Vinayak Kulkarni",
    "email": "inbox.vinayak@gmail.com",
    "url": "https://vinayakkulkarni.dev"
  },
  "keywords": [
    "treeview",
    "v-tree",
    "vuejs",
    "vue-router",
    "vue"
  ],
  "license": "MIT",
  "issues": "https://github.com/vinayakkulkarni/v-tree/issues",
  "bugs": {
    "url": "https://github.com/vinayakkulkarni/v-tree/issues"
  },
  "homepage": "https://v-tree.netlify.app"
}
